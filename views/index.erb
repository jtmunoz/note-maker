<h2>All Notes</h2>
<ul>
<% @notes.each do |note| %>
 <li>
   <a href="/note/<%= note.id %>"><%= note.title %></a> | <%= note.created_at %>
 </li>
<% end %>
</ul>
<h2>Add Note</h2>
<form method="note" action="/note">
		<label for="title">Title: </label>
   <input type="text" name="title">
   <label for="body">Body: </label>
   <input type="text" name="body">
   <input type ="submit" value="Add Note">
</form>